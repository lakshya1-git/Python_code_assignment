Answer 5:
my_list = [5, 2, 8, 1, 10]
largest_number = float('-inf')  # Initialize largest_number as negative infinity
second_largest_number = float('-inf') 

for number in my_list:
    if number > largest_number:
        second_largest_number = largest_number
        largest_number = number
    elif number > second_largest_number and number != largest_number:
        second_largest_number = number

print("The second largest number in the list is:", second_largest_number)

Answer 6: def find_n_largest_elements(numbers, n):
    largest_elements = []
    numbers.sort(reverse=True)  # Sort the list in descending order

    for i in range(n):
        largest_elements.append(numbers[i])

    return largest_elements


my_list = [5, 2, 8, 1, 10, 7, 3]
N = 3  # Number of largest elements to find

result = find_n_largest_elements(my_list, N)
print(f"The {N} largest elements in the list are:", result)


Answer 7 : def print_even_numbers(numbers):
    even_numbers = []

    for number in numbers:
        if number % 2 == 0:
            even_numbers.append(number)

    print("Even numbers in the list:", even_numbers)


my_list = [5, 2, 8, 1, 10, 7, 3]

print_even_numbers(my_list)


Answer 8:


def print_even_numbers(numbers):
    even_numbers = []

    for number in numbers:
        if number % 2 !== 0:
            even_numbers.append(number)

    print("Even numbers in the list:", even_numbers)


my_list = [5, 2, 8, 1, 10, 7, 3]

print_even_numbers(my_list)

Answer 9: 

def clone_list(original_list):
    cloned_list = original_list.copy()  # Using the copy() method to create a shallow copy of the original list
    return cloned_list


my_list = [1, 2, 3, 4, 5]

cloned_list = clone_list(my_list)
print("Original List:", my_list)
print("Cloned List:", cloned_list)


Answer10:

def count_occurrences(numbers, element):
    count = 0

    for number in numbers:
        if number == element:
            count += 1

    return count


my_list = [1, 2, 3, 2, 4, 2, 5, 2]
element_to_count = 2

occurrences = count_occurrences(my_list, element_to_count)
print(f"The element {element_to_count} occurs {occurrences} time(s) in the list.")


